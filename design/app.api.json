                { "comment": "总盈利", "javaType": "String", "name": "totalProfit", "required": true },
                { "comment": "均单盈利", "javaType": "String", "name": "singleProfit", "required": true },
                { "comment": "成功率", "javaType": "String", "name": "successRate", "required": true }
